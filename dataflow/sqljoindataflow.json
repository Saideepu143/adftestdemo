{
	"name": "sqljoindataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sqldataset",
						"type": "DatasetReference"
					},
					"name": "sqltable1"
				},
				{
					"dataset": {
						"referenceName": "sqldbdataset",
						"type": "DatasetReference"
					},
					"name": "sqltable2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "outputdataset",
						"type": "DatasetReference"
					},
					"name": "sqljoinsinkdata"
				}
			],
			"transformations": [
				{
					"name": "jointable"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Id as integer,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Email as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sqltable1",
				"source(output(",
				"          empid as integer,",
				"          CName as string,",
				"          department as string,",
				"          location as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sqltable2",
				"sqltable1, sqltable2 join(Id == empid,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> jointable",
				"jointable sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sqljoinsinkdata"
			]
		}
	}
}